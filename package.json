{
  "name": "hackernews-react-apollo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "@types/jest": "24.0.0",
    "@types/node": "12.0.0",
    "@types/react": "16.9.0",
    "@types/react-dom": "16.9.0",
    "apollo": "2.27.4",
    "apollo-boost": "0.4.7",
    "graphql": "15.0.0",
    "node-sass": "4.14.1",
    "react": "16.13.1",
    "react-apollo": "3.1.5",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.1",
    "tachyons": "4.12.0",
    "tachyons-sass": "4.9.5",
    "typescript": "3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gql-codegen": "apollo client:codegen --target=typescript --localSchemaFile=./schema.json --globalTypesFile=./src/globalTypes.ts --config=./apollo.config.js --passthroughCustomScalars",
    "gql-clean": "git diff-index --quiet HEAD -- && git clean -dfx src || echo 'Changes found, stash them before running.'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
